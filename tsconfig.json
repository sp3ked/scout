{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/lens.jsx",
    "app/(tabs)/items.jsx",
    "app/login.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/index.jsx"
  ]
}
